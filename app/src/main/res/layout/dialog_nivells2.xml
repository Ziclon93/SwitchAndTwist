<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/Layout_levels2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/fondo_nivells"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">


        <Button
            android:id="@+id/boto_close_x2"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="11dp"
            android:layout_marginRight="11dp"
            android:background="@drawable/boto_close_x" />

        <Button
            android:id="@+id/Forward2"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignBaseline="@+id/Next2"
            android:layout_alignBottom="@+id/Next2"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:background="@drawable/boto_forward" />

        <Button
            android:id="@+id/Next2"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginTop="160dp"
            android:background="@drawable/boto_next" />

        <Button
            android:id="@+id/level6"
            android:layout_width="56dp"
            android:layout_height="57dp"
            android:layout_marginTop="80dp"
            android:layout_toEndOf="@+id/Forward2"
            android:layout_toRightOf="@+id/Forward2"
            android:background="@drawable/level6" />

        <Button
            android:id="@+id/level8"
            android:layout_width="56dp"
            android:layout_height="57dp"
            android:layout_alignBaseline="@+id/level7"
            android:layout_alignBottom="@+id/level7"
            android:layout_toStartOf="@+id/Next2"
            android:layout_toLeftOf="@+id/Next2"
            android:background="@drawable/level8" />

        <Button
            android:id="@+id/level7"
            android:layout_width="56dp"
            android:layout_height="57dp"
            android:layout_below="@+id/boto_close_x2"
            android:layout_alignBaseline="@+id/level6"
            android:layout_alignBottom="@+id/level6"
            android:layout_centerHorizontal="true"
            android:background="@drawable/level7" />

        <Button
            android:id="@+id/level9"
            android:layout_width="56dp"
            android:layout_height="57dp"
            android:layout_alignTop="@+id/Forward2"
            android:layout_marginStart="45dp"
            android:layout_marginLeft="45dp"
            android:layout_toEndOf="@+id/Forward2"
            android:layout_toRightOf="@+id/Forward2"
            android:background="@drawable/level9" />

        <Button
            android:id="@+id/level10"
            android:layout_width="56dp"
            android:layout_height="57dp"
            android:layout_alignTop="@+id/Forward2"
            android:layout_marginStart="40dp"
            android:layout_marginLeft="45dp"
            android:layout_toEndOf="@+id/level9"
            android:layout_toRightOf="@+id/level9"
            android:background="@drawable/level10" />


        <ImageView
            android:id="@+id/slider2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/level9"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="29dp"
            app:srcCompat="@drawable/slider2" />

        <Button
            android:id="@+id/Home2"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:layout_below="@+id/slider2"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="15dp"
            android:background="@drawable/boto_home" />

        <ImageView
            android:id="@+id/tabac_lvl6"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_below="@+id/level6"
            android:layout_alignEnd="@+id/level6"
            android:layout_alignRight="@+id/level6"
            android:layout_marginEnd="11dp"
            android:layout_marginRight="11dp"
            app:srcCompat="@drawable/tobacco" />

        <ImageView
            android:id="@+id/tabac_lvl7"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_below="@+id/level7"
            android:layout_alignEnd="@+id/level7"
            android:layout_alignRight="@+id/level7"
            android:layout_marginEnd="11dp"
            android:layout_marginRight="11dp"
            android:layout_centerHorizontal="true"
            app:srcCompat="@drawable/tobacco" />

        <ImageView
            android:id="@+id/tabac_lvl8"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_below="@+id/level8"
            android:layout_toStartOf="@+id/boto_close_x2"
            android:layout_toLeftOf="@+id/boto_close_x2"
            app:srcCompat="@drawable/tobacco" />

        <ImageView
            android:id="@+id/tabac_lvl9"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_below="@+id/level9"
            android:layout_alignEnd="@+id/level9"
            android:layout_alignRight="@+id/level9"
            android:layout_marginEnd="12dp"
            android:layout_marginRight="12dp"
            app:srcCompat="@drawable/tobacco" />

        <ImageView
            android:id="@+id/tabac_lvl10"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_below="@+id/level10"
            android:layout_alignEnd="@+id/level10"
            android:layout_alignRight="@+id/level10"
            android:layout_marginEnd="12dp"
            android:layout_marginRight="12dp"
            app:srcCompat="@drawable/tobacco" />


    </RelativeLayout>
</android.support.constraint.ConstraintLayout>